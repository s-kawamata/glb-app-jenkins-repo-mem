def shResult() {
    script {
        sh script:'python3 scripts/kintai_change_start_syussya.py', returnStdout: true ${departure_station} ${destination_station}
    }
}

pipeline {
  agent any

  stages {
    stage('勤務開始(出社)') {
      steps {
              script {
                result = shResult()
              }
      }
    }
  } 
  
