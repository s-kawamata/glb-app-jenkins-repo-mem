def shResult() {
    script {
        sh script:'python3 scripts/kintai_change_start_remote.py', returnStdout: true
    }
}

pipeline {
  agent any

  stages {
    stage('勤務開始(リモワ)') {
      steps {
              script {
                result = shResult()
              }
      }
    }
  } 
  
