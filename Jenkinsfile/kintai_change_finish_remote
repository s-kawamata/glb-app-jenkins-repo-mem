def shResult() {
    script {
        sh script:'python3 scripts/kintai_change_finish_remote.py', returnStdout: true
    }
}

pipeline {
  agent any

  stages {
    stage('リモワ完了') {
      steps {
              script {
                result = shResult()
              }
      }
    }
  } 
  
   post {
         success {
             slackSend  channel: "#akatsuka_test", teamDomain: "all-apc", tokenCredentialId: "slack_webhook_token_mem", color: 'good', message: "リモワ完了します"
         }
   }
}
